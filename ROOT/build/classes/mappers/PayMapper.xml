<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.madforgolf.mapper.PayMapper">
	
	
	<insert id="payInsert">
		insert into mfg_pay(prod_num, price, payment)  
		values(#{prod_num}, #{price}, #{payment})
	</insert>

</mapper>